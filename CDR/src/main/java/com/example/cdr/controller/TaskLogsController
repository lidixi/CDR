package com.example.cdr.controller;

import com.example.cdr.service.TaskLogsService;

import org.springframework.web.bind.annotation.*;

import java.util.*;

@RestController
public class TaskLogsController {

    private final TaskLogsService taskLogsService;
    public TaskLogsController(TaskLogsService taskLogsService) {
        this.taskLogsService = taskLogsService;
    }


    @GetMapping("/getTaskLogs")
    public List<Map<String, Object>> getTaskLogs() {
        return taskLogsService.getTaskLogs();
    }
}